<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/layout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".RandomizedView">


    <ImageView
        android:id="@+id/ivTop"
        android:layout_width="120dp"
        android:layout_height="120dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="25dp"
        app:layout_constraintStart_toEndOf="@+id/ivBracelet"
        app:layout_constraintTop_toBottomOf="@+id/ivHeadwear"
        tools:srcCompat="@tools:sample/avatars" />

    <ImageView
        android:id="@+id/ivBottom"
        android:layout_width="120dp"
        android:layout_height="120dp"
        android:layout_marginTop="13dp"
        app:layout_constraintStart_toStartOf="@+id/ivTop"
        app:layout_constraintTop_toBottomOf="@+id/ivTop"
        tools:srcCompat="@tools:sample/avatars" />

    <ImageView
        android:id="@+id/ivShoes"
        android:layout_width="120dp"
        android:layout_height="120dp"
        android:layout_marginTop="13dp"
        app:layout_constraintStart_toStartOf="@+id/ivBottom"
        app:layout_constraintTop_toBottomOf="@+id/ivBottom"
        tools:srcCompat="@tools:sample/avatars" />

    <ImageView
        android:id="@+id/ivHeadwear"
        android:layout_width="120dp"
        android:layout_height="120dp"
        android:layout_marginStart="143dp"
        android:layout_marginTop="17dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:srcCompat="@tools:sample/avatars" />

    <ImageView
        android:id="@+id/ivBracelet"
        android:layout_width="120dp"
        android:layout_height="120dp"
        android:layout_marginStart="6dp"
        android:layout_marginTop="162dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:srcCompat="@tools:sample/avatars" />

    <ImageView
        android:id="@+id/ivNecklace"
        android:layout_width="120dp"
        android:layout_height="120dp"
        android:layout_marginTop="13dp"
        app:layout_constraintStart_toStartOf="@+id/ivBracelet"
        app:layout_constraintTop_toBottomOf="@+id/ivBracelet"
        tools:srcCompat="@tools:sample/avatars" />

    <ImageView
        android:id="@+id/ivEarrings"
        android:layout_width="120dp"
        android:layout_height="120dp"
        android:layout_marginTop="13dp"
        app:layout_constraintStart_toStartOf="@+id/ivNecklace"
        app:layout_constraintTop_toBottomOf="@+id/ivNecklace"
        tools:srcCompat="@tools:sample/avatars" />

    <ImageView
        android:id="@+id/ivHandheld"
        android:layout_width="120dp"
        android:layout_height="120dp"
        android:layout_marginStart="13dp"
        android:layout_marginTop="160dp"
        app:layout_constraintStart_toEndOf="@+id/ivTop"
        app:layout_constraintTop_toTopOf="parent"
        tools:srcCompat="@tools:sample/avatars" />

    <ImageView
        android:id="@+id/ivOverwear"
        android:layout_width="120dp"
        android:layout_height="120dp"
        android:layout_marginBottom="3dp"
        app:layout_constraintBottom_toBottomOf="@+id/ivBottom"
        app:layout_constraintStart_toStartOf="@+id/ivHandheld"
        tools:srcCompat="@tools:sample/avatars" />

    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/rvRandomTops"
        android:layout_width="120dp"
        android:layout_height="120dp"
        android:layout_centerHorizontal="true"
        android:layout_centerVertical="true"
        android:layout_marginStart="1dp"
        app:layout_constraintBottom_toBottomOf="@+id/ivTop"
        app:layout_constraintStart_toStartOf="@+id/ivTop"
        app:layout_constraintTop_toTopOf="@+id/ivTop" />

    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/rvRandomBottoms"
        android:layout_width="120dp"
        android:layout_height="120dp"
        android:layout_centerHorizontal="true"
        android:layout_centerVertical="true"
        app:layout_constraintStart_toStartOf="@+id/ivBottom"
        app:layout_constraintTop_toTopOf="@+id/ivBottom" />

    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/rvRandomHeadwear"
        android:layout_width="120dp"
        android:layout_height="120dp"
        android:layout_centerHorizontal="true"
        android:layout_centerVertical="true"
        app:layout_constraintStart_toStartOf="@+id/ivHeadwear"
        app:layout_constraintTop_toTopOf="@+id/ivHeadwear" />

    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/rvRandomShoes"
        android:layout_width="120dp"
        android:layout_height="120dp"
        android:layout_centerHorizontal="true"
        android:layout_centerVertical="true"
        android:layout_marginTop="132dp"
        app:layout_constraintStart_toStartOf="@+id/ivBottom"
        app:layout_constraintTop_toTopOf="@+id/ivBottom" />

    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/rvRandomOverwear"
        android:layout_width="120dp"
        android:layout_height="120dp"
        android:layout_centerHorizontal="true"
        android:layout_centerVertical="true"
        app:layout_constraintStart_toStartOf="@+id/ivOverwear"
        app:layout_constraintTop_toTopOf="@+id/ivOverwear" />


    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/rvRandomHandheld"
        android:layout_width="120dp"
        android:layout_height="120dp"
        android:layout_centerHorizontal="true"
        android:layout_centerVertical="true"
        app:layout_constraintStart_toStartOf="@+id/ivHandheld"
        app:layout_constraintTop_toTopOf="@+id/ivHandheld" />

    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/rvRandomBracelet"
        android:layout_width="120dp"
        android:layout_height="120dp"
        android:layout_centerHorizontal="true"
        android:layout_centerVertical="true"
        app:layout_constraintStart_toStartOf="@+id/ivBracelet"
        app:layout_constraintTop_toTopOf="@+id/ivBracelet" />

    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/rvRandomNecklace"
        android:layout_width="120dp"
        android:layout_height="120dp"
        android:layout_centerHorizontal="true"
        android:layout_centerVertical="true"
        app:layout_constraintStart_toStartOf="@+id/ivNecklace"
        app:layout_constraintTop_toTopOf="@+id/ivNecklace" />

    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/rvRandomEarrings"
        android:layout_width="120dp"
        android:layout_height="120dp"
        android:layout_centerHorizontal="true"
        android:layout_centerVertical="true"
        app:layout_constraintStart_toStartOf="@+id/ivEarrings"
        app:layout_constraintTop_toTopOf="@+id/ivEarrings" />

    <ImageButton
        android:id="@+id/ibAdd"
        android:layout_width="58dp"
        android:layout_height="55dp"
        android:layout_marginTop="568dp"
        android:background="@drawable/add"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />


</androidx.constraintlayout.widget.ConstraintLayout>